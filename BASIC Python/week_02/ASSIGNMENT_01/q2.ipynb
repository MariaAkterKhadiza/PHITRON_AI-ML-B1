    "def safe_Divide(a,b):\n",
    "    try:\n",
    "        result = a/b\n",
    "        print(result)\n",
    "    except  ZeroDivisionError:\n",
    "        print(\"Division by zero\")\n",
    "\n",
    "a, b = map(int, input().split())\n",
    "safe_Divide(a, b)"
   ]
